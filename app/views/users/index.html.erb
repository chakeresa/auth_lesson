<% if flash[:notice] %>
  <%= flash[:notice] %>
<% end %>
<%= link_to "Register as User", new_user_path %>
<% if session[:current_user_email] %>
  <%= button_to 'Log Out', "/login", method: "delete" %>
<% else %>
  <%= link_to "Log In", "/login" %>
<% end %>
<% @users.each do |user| %>
  <div id="user-<%= user.id %>">
    <h3><%= user.name %></h3>
    <p><%= user.email %></p>
  </div>
<% end %>